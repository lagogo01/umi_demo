    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.12",
    "@turf/turf": "^5.1.6",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.19",
    "antd": "^4.5.0",
    "axios": "^0.19.1",
    "cesium": "^1.68.0",
    "classnames": "^2.2.6",
    "echarts": "^4.7.0",
    "events": "^3.1.0",
    "jquery": "^3.4.1",
    "jsencrypt": "^3.0.0-rc.1",
    "lint-staged": "^10.0.7",
    "moment": "^2.24.0",
    "nsc-earth": "^0.0.37",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^1.19.1",
    "qs": "^6.9.1",
    "rc-menu": "^7.5.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-reverse-portal": "^2.0.0",
    "react-unity-webgl": "^7.1.10",
    "react-zmage": "^0.8.5-beta.31",
    "swiper": "^5.4.3",
    "umi": "^3.2.19",
    "yorkie": "^2.0.0"
  }
